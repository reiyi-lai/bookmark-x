{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:extension": "cd chrome-extension && vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "supabase:login": "supabase login",
    "supabase:init": "supabase init",
    "supabase:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "supabase:migration": "supabase db diff --file=migration_$(date +%Y%m%d_%H%M%S)",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:reset": "supabase db reset",
    "supabase:types": "supabase gen types typescript --local > shared/supabase-types.ts",
    "supabase:types:remote": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > shared/supabase-types.ts",
    "schema:update": "npm run supabase:types && npm run schema:integrate",
    "schema:integrate": "echo 'Types generated! Update shared/schema.ts with generated types.'"
  },
